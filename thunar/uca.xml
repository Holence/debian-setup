<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1737219515131061-1</unique-id>
	<command>if [ -d %f ]; then exo-open --working-directory %f --launch TerminalEmulator; else exo-open --working-directory %d --launch TerminalEmulator; fi</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>vscode</icon>
	<name>Open VSCode Here</name>
	<submenu></submenu>
	<unique-id>1737376255565331-1</unique-id>
	<command>code %f</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>image-x-generic</icon>
	<name>Convert to JPEG</name>
	<submenu></submenu>
	<unique-id>1737376480170674-2</unique-id>
	<command>for f in %F ; do convert &quot;$f&quot; -quality 95 &quot;$(basename &quot;$f&quot; .png).jpg&quot; ; done</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<image-files/>
</action>
<action>
	<icon>avidemux</icon>
	<name>Open Slide Show</name>
	<submenu></submenu>
	<unique-id>1737393447076097-1</unique-id>
	<command>xnview %f</command>
	<description>xnview&apos;s slide show</description>
	<range>*</range>
	<patterns>*.sld</patterns>
	<text-files/>
</action>
<action>
	<icon>text-x-python</icon>
	<name>Exec Python Script Here</name>
	<submenu></submenu>
	<unique-id>1742133603203534-1</unique-id>
	<command>f=%f; cmd=&quot;python3 \&quot;$f\&quot;; exec bash&quot;; x-terminal-emulator -e &quot;bash -c &apos;$cmd&apos;&quot;</command>
	<description></description>
	<range></range>
	<patterns>*.py</patterns>
	<text-files/>
</action>
<action>
	<icon>application-x-shellscript</icon>
	<name>Exec in Shell</name>
	<submenu></submenu>
	<unique-id>1756025579832168-1</unique-id>
	<command>x-terminal-emulator -e %f</command>
	<description></description>
	<range>1</range>
	<patterns>*</patterns>
	<other-files/>
</action>
<action>
	<icon>add-folder-to-archive</icon>
	<name>Zip each folder</name>
	<submenu></submenu>
	<unique-id>1759488757808849-1</unique-id>
	<command>for f in %F ; do zip -FS -r &quot;$f&quot;.zip &quot;$f&quot; ; done</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
</actions>
